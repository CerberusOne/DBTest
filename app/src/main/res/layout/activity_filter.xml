<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.aingaran.dbtest.FilterActivity">

    <EditText
        android:id="@+id/longStart"
        android:layout_width="113dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="Start"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="329dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="89dp"
        android:layout_height="42dp"
        android:text="Long"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="329dp" />

    <EditText
        android:id="@+id/longEnd"
        android:layout_width="127dp"
        android:layout_height="42dp"
        android:ems="10"
        android:hint="End"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="241dp"
        tools:layout_editor_absoluteY="329dp" />

    <EditText
        android:id="@+id/dayStart"
        android:layout_width="113dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="Start"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="233dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="89dp"
        android:layout_height="37dp"
        android:text="Day"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="237dp" />

    <EditText
        android:id="@+id/dayEnd"
        android:layout_width="127dp"
        android:layout_height="42dp"
        android:ems="10"
        android:hint="End"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="241dp"
        tools:layout_editor_absoluteY="234dp" />

    <EditText
        android:id="@+id/latStart"
        android:layout_width="113dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="Start"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="285dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="89dp"
        android:layout_height="42dp"
        android:text="Lat"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="285dp" />

    <EditText
        android:id="@+id/latEnd"
        android:layout_width="127dp"
        android:layout_height="42dp"
        android:ems="10"
        android:hint="End"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="241dp"
        tools:layout_editor_absoluteY="285dp" />

    <EditText
        android:id="@+id/monthStart"
        android:layout_width="113dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="Start"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="191dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="89dp"
        android:layout_height="42dp"
        android:text="Month"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="191dp" />

    <EditText
        android:id="@+id/monthEnd"
        android:layout_width="127dp"
        android:layout_height="42dp"
        android:ems="10"
        android:hint="End"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="241dp"
        tools:layout_editor_absoluteY="191dp" />

    <EditText
        android:id="@+id/yearEnd"
        android:layout_width="127dp"
        android:layout_height="42dp"
        android:ems="10"
        android:hint="End"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="241dp"
        tools:layout_editor_absoluteY="147dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="89dp"
        android:layout_height="47dp"
        android:text="Caption"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="88dp" />

    <EditText
        android:id="@+id/caption"
        android:layout_width="259dp"
        android:layout_height="47dp"
        android:ems="10"
        android:hint="Caption"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="109dp"
        tools:layout_editor_absoluteY="88dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Filter Options:"
        android:textSize="30sp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="16dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Filter"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="148dp" />

    <EditText
        android:id="@+id/yearStart"
        android:layout_width="113dp"
        android:layout_height="44dp"
        android:ems="10"
        android:hint="Start"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="147dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="89dp"
        android:layout_height="37dp"
        android:text="Year"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="149dp" />

</android.support.constraint.ConstraintLayout>
