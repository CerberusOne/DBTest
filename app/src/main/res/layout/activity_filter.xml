<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.aingaran.dbtest.FilterActivity">

    <EditText
        android:id="@+id/longStart"
        android:layout_width="113dp"
        android:layout_height="44dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="Start"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toStartOf="@+id/longEnd" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="89dp"
        android:layout_height="42dp"
        android:layout_marginBottom="64dp"
        android:layout_marginStart="8dp"
        android:text="Long"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/longEnd"
        android:layout_width="127dp"
        android:layout_height="42dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:hint="End"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/dayStart"
        android:layout_width="113dp"
        android:layout_height="44dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="Start"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/latStart"
        app:layout_constraintEnd_toStartOf="@+id/dayEnd" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="89dp"
        android:layout_height="37dp"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="8dp"
        android:text="Day"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/dayEnd"
        android:layout_width="127dp"
        android:layout_height="42dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:hint="End"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/latEnd"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/latStart"
        android:layout_width="113dp"
        android:layout_height="44dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="Start"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/longStart"
        app:layout_constraintEnd_toStartOf="@+id/latEnd" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="89dp"
        android:layout_height="42dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="8dp"
        android:text="Lat"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/latEnd"
        android:layout_width="127dp"
        android:layout_height="42dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:hint="End"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/longEnd"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/monthStart"
        android:layout_width="114dp"
        android:layout_height="44dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="Start"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/dayStart"
        app:layout_constraintEnd_toStartOf="@+id/monthEnd" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="89dp"
        android:layout_height="42dp"
        android:layout_marginBottom="12dp"
        android:layout_marginStart="8dp"
        android:text="Month"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/monthEnd"
        android:layout_width="126dp"
        android:layout_height="42dp"
        android:layout_marginBottom="12dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:hint="End"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/dayEnd"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/yearEnd"
        android:layout_width="127dp"
        android:layout_height="42dp"
        android:layout_marginBottom="12dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:hint="End"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/monthEnd"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="89dp"
        android:layout_height="47dp"
        android:layout_marginBottom="12dp"
        android:layout_marginStart="8dp"
        android:text="Caption"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/caption"
        android:layout_width="259dp"
        android:layout_height="47dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="124dp"
        android:ems="10"
        android:hint="Caption"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/yearEnd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.666" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:text="Filter Options:"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:onClick="filter"
        android:text="Filter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/yearStart"
        android:layout_width="113dp"
        android:layout_height="44dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:ems="10"
        android:hint="Start"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/monthStart"
        app:layout_constraintEnd_toStartOf="@+id/yearEnd" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="89dp"
        android:layout_height="37dp"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="8dp"
        android:text="Year"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>
